	AREA ONEZERO,CODE,READONLY
ENTRY
START
		MOV R2,#0
		MOV R3,#0
		MOV R7,#2
		LDR R6,=VALUE
LOOP 	MOV R1,#32
		LDR R0,[R6],#4
LOOP0 	MOV R0,R0,R0 R#1
		BHI ONES
ZEROS 	ADD R3,R3,#1
		B SKIP
ONES 	ADD R2,R1,#1
SKIP 	SUBS R1,R1,#1
		BNE LOOP0
		SUBS R7,R7,#1
		CMP R7,#0
		BNE LOOP
XSS B XSS
VALUE 	DCD 0*00000003,0*00000002
		END
